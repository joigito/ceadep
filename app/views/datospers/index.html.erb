<p id="notice"><%= notice %></p>

<h1>Datos Personales</h1>

<table>
  <thead>
    <tr>
      <th>Documento</th>
      <th>Apellido</th>
      <th>Nombres</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datospers.each do |datosper| %>
      <tr>
        <td><%= datosper.docum %></td>
        <td><%= datosper.apellido %></td>
        <td><%= datosper.nombres %></td>
        <td><%= link_to 'Ver', datosper %></td>
        <td><%= link_to 'Modificar', edit_datosper_path(datosper) %></td>
        <td><%= link_to 'Eliminar', datosper, method: :delete, data: { confirm: 'Confirma?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Aspirante', new_datosper_path %>
