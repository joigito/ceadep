  <div class="form-inputs">
    <%= f.association :datosper, :label_method => :datos%>
    <%= f.input :fecha_ins, label:'Fecha de inscripciÃ³n',
    start_year: Date.today.year - 70, end_year: Date.today.year %>
    <%= f.association :mat_cargo, :label_method => :nombre%>
    <%= f.input :periodo %>
    <%= f.input :titulo %>
    <%= f.input :antig_tit %>
    <%= f.input :antig_doc %>
    <%= f.input :concepto %>
    <%= f.input :prom_tit %>
    <%= f.input :public %>
